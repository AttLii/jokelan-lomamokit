{
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "contact-form/index.ts": {
      "memory": 512,
      "maxDuration": 10
    }
  }
}
