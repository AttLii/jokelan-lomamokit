{
  "trailingSlash": false,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "api/contact-form.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "excludeFiles": "./tsconfig.json",
      "includeFiles": "./api/tsconfig.json"
    }
  }
}
